<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="spring-boot" parallel="tests" thread-count="3">
    <test name="GTest_1">
        <classes>
            <class name="com.udemy.spring.cucumberwithspringboot.google.GoogleTest1" />
            <class name="com.udemy.spring.cucumberwithspringboot.google.GoogleTest2" />
        </classes>
    </test>
    <test name="GTest_2">
        <classes>
            <class name="com.udemy.spring.cucumberwithspringboot.google.GoogleTest3" />
            <class name="com.udemy.spring.cucumberwithspringboot.google.GoogleTest4" />
        </classes>
    </test>
    <test name="RTest_1">
        <classes>
            <class name="com.udemy.spring.cucumberwithspringboot.redbus.RedBusTest" />
        </classes>
    </test>
    <test name="VTest_1">
        <parameter name="dobStartFrom" value="1995-01-01"/>
        <parameter name="dobEndTo" value="1997-01-01"/>
        <classes>
            <class name="com.udemy.spring.cucumberwithspringboot.spring_boot_data_JPA_page.CustomerVisaTest" />
        </classes>
    </test>
</suite>